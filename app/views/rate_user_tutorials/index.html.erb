<div class="row">
            <div class="col-lg-12">
              <h1 class="page-header">Listing Rate User Tutorials
                    
                </h1>
            </div>
        </div>

<!-- <%= url_for :controller => 'tutorials', :action => 'myindex' %> -->
 <div class="col-md-12"> 
<div class="well">
<table style="margin-left:30px">
  <thead>
    <tr>
      <th style="padding-right:20px"><h1 class="page-header"><small> Tutorial</small></h1></th>
      <th style="padding-right:20px"><h1 class="page-header"><small> User</small></h1></th>
      <th style="padding-right:20px"><h1 class="page-header"><small> Score</small></h1></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rate_user_tutorials.each do |rate_user_tutorial| %>
		<%if rate_user_tutorial.tutorial && rate_user_tutorial.user%>
      <tr>
        <td style="padding-right:20px; text-align:left"><%=link_to rate_user_tutorial.tutorial.title,  rate_user_tutorial.tutorial%></td>
        <td style="padding-right:20px;text-align:center"><%= rate_user_tutorial.user.user_name %></td>
        <td style="padding-right:20px; text-align:center"><%= rate_user_tutorial.score %></td>
        
    <%if current_user && current_user.user_name=='admin'%>

        <td style="padding-right:20px"><%= link_to 'Show', rate_user_tutorial %></td>
        <td style="padding-right:20px"><%= link_to 'Edit', edit_rate_user_tutorial_path(rate_user_tutorial) %></td>
        <td style="padding-right:20px"><%= link_to 'Destroy', rate_user_tutorial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
		<%end%>
    <% end %>
     <% end %>
  </tbody>
</table>

</div>
